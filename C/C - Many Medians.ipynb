   "source": [
    "import numpy as np\n",
    "n = int(input())\n",
    "sx = np.array([int(i) for i in input().split()])\n",
    "x = np.sort(sx)\n",
    "\n",
    "cent = int(np.ceil(n/2)) - 1\n",
    "\n",
    "cent_n = x[cent]\n",
    "\n",
    "for i in range(n):\n",
    "    if sx[i] <= cent_n:\n",
    "        print(x[cent+1])\n",
    "    else:\n",
    "        print(x[cent])\n",
    "        \n"
