    "import numpy as np\n",
    "\n",
    "N = int(input())\n",
    "N_original = N\n",
    "N_a_m = int(np.floor(N**(0.5)))\n",
    "\n",
    "\n",
    "for i in range(1,N_a_m+1):\n",
    "    j = N_a_m+1 - i\n",
    "    if N%j == 0:\n",
    "        N_A = j\n",
    "        break\n",
    "\n",
    "\n",
    "    \n",
    "N_B = len(str(int(N_original/N_A)))\n",
    "        \n",
    "print(N_B)"
   ]
