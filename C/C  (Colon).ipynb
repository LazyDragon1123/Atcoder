    "import math\n",
    "a,b,h,m = [int(i) for i in input().split()]\n",
    "\n",
    "theta_a = (math.pi/6)*(h + m/60) % (2*math.pi)\n",
    "theta_b = 2*math.pi*m/60  % (2*math.pi)\n",
    "diff = abs(theta_a-theta_b)\n",
    "if diff > math.pi:\n",
    "    diff = 2*math.pi - diff\n",
    "\n",
    "c = (a**2+b**2 - 2*a*b*math.cos(diff))**0.5\n",
    "print(c)"
   ]
  }
