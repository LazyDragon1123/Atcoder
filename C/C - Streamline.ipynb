   "source": [
    "import numpy as np\n",
    "\n",
    "n, m = map(int, input().split())\n",
    "x = [int(i) for i in input().split()]\n",
    "\n",
    "rest = m - n\n",
    "\n",
    "x = np.sort(x)\n",
    "dis = np.ones(m-1)\n",
    "\n",
    "for i in range(m-1):\n",
    "    dis[i] = x[i+1] - x[i]\n",
    "    \n",
    "dis = np.sort(dis)\n",
    "\n",
    "ans = 0\n",
    "\n",
    "for i in range(rest):\n",
    "    ans += dis[i]\n",
    "    \n",
    "print(int(ans))"
   ]
