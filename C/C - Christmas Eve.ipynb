   "source": [
    "import numpy as np\n",
    "\n",
    "n, k = [int(i) for i in input().split()]\n",
    "\n",
    "h = np.ones(n)\n",
    "\n",
    "for i in range(n):\n",
    "    h[i] = int(input())\n",
    "    \n",
    "h = np.sort(h)\n",
    "\n",
    "ans = float('inf')\n",
    "\n",
    "for i in range(n-k+1):\n",
    "    cand = h[i+k-1] - h[i]\n",
    "    if cand < ans:\n",
    "        ans = cand\n",
    "    \n",
    "print(int(ans))"
   ]
