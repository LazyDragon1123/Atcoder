   "source": [
    "N, T = [int(i) for i in input().split()]\n",
    "\n",
    "t = [int(i) for i in input().split()]\n",
    "\n",
    "ans = N*T\n",
    "\n",
    "for i in range(N-1):\n",
    "    time_gap = t[i+1] - t[i] \n",
    "    if time_gap > T:\n",
    "        loss_i =0\n",
    "    else:\n",
    "        loss_i = T - time_gap\n",
    "    ans -= loss_i\n",
    "    \n",
    "print(int(ans))"
   ]
  }
