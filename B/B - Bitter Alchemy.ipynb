    "n,x = [int(i) for i in input().split()]\n",
    "\n",
    "mind = float('inf')\n",
    "for i in range(n):\n",
    "    m_ = int(input())\n",
    "    x -= m_\n",
    "    if mind > m_:\n",
    "        mind = m_\n",
    "ans = n + int(x // mind)\n",
    "print(ans)"
