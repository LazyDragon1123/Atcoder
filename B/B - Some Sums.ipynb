    "import numpy as np\n",
    "\n",
    "n, a, b = [int(i) for i in input().split()]\n",
    "\n",
    "ans = 0\n",
    "\n",
    "def check(m):\n",
    "    mst = str(m)\n",
    "    res = 0\n",
    "    for i in mst:\n",
    "        res += int(i)\n",
    "        \n",
    "    return res\n",
    "\n",
    "\n",
    "for num in range(1,n+1):\n",
    "    if a<= check(num) and b>=check(num):\n",
    "        ans += num\n",
    "    \n",
    "print(ans)"
