    "import numpy as np\n",
    "\n",
    "N = int(input())\n",
    "\n",
    "T, A = [int(i) for i in input().split()]\n",
    "\n",
    "H = np.array([int(i) for i in input().split()])\n",
    "\n",
    "temp = T - 0.006*H\n",
    "\n",
    "temp_def = temp-A\n",
    "\n",
    "temp_target = min(abs(temp_def))\n",
    "\n",
    "ans = int(np.where(abs(temp_def) == temp_target)[0]) +1\n",
    "\n",
    "\n",
    "print(ans)"
   ]
  }
