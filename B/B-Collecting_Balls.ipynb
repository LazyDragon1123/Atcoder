    "n = int(input())\n",
    "k = int(input())\n",
    "x = np.array([int(i) for i in input().split()])\n",
    "\n",
    "diff = np.array(abs(x - k))\n",
    "ans = 0\n",
    "\n",
    "for i in range(n):\n",
    "    ans += 2*min(x[i],diff[i])\n",
    "    \n",
    "print(int(ans))"
