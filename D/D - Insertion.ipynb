   "source": [
    "n = int(input())\n",
    "\n",
    "s =str(input())\n",
    "\n",
    "\n",
    "left_save = 0\n",
    "dept=0\n",
    "\n",
    "for i in range(len(s)):\n",
    "    if s[i] ==')':\n",
    "        if left_save == 0:\n",
    "            dept +=1\n",
    "        else:\n",
    "            left_save-=1\n",
    "            \n",
    "    else:\n",
    "        left_save+=1\n",
    "\n",
    "ans = '('*dept + s + ')'*left_save\n",
    "\n",
    "print(ans)"
   ]
  }
