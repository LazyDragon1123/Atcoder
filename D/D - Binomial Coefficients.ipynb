   "source": [
    "n = int(input())\n",
    "a = [int(i) for i in input().split()]\n",
    "\n",
    "a.sort()\n",
    "\n",
    "max_a = a[-1]\n",
    "\n",
    "max_b = 1\n",
    "\n",
    "for i in range(n):\n",
    "    if a[i] <= int((max_a+1)//2):\n",
    "        max_b = a[i]\n",
    "        \n",
    "    else:\n",
    "        break\n",
    "ans = str(max_a) + str(' ') + str(max_b)\n",
    "\n",
    "print(ans)\n",
    "    "
   ]
